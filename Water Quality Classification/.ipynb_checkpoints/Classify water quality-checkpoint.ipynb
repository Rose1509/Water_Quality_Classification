    "def predict_water_rf(rf_model):\n",
    "    \"\"\"\n",
    "    Predict water quality (Safe/Unsafe) using user input for Random Forest\n",
    "    \"\"\"\n",
    "    features = ['Lat', 'Lon', 'Temperature', 'D.O', 'pH', \n",
    "                'B.O.D', 'Nitrate', 'Fecalcaliform', 'Totalcaliform', 'Conductivity']\n",
    "    \n",
    "    print(\"Enter water quality parameters:\")\n",
    "\n",
    "    user_data = {}\n",
    "    try:\n",
    "        for feature in features:\n",
    "            value = float(input(f\"{feature}: \"))\n",
    "            user_data[feature] = value\n",
    "    except ValueError:\n",
    "        print(\"Invalid input! Please enter numeric values only.\")\n",
    "        return\n",
    "\n",
    "    # Convert to DataFrame\n",
    "    X_new = pd.DataFrame([user_data], columns=features)\n",
    "\n",
    "    # Predict\n",
    "    pred = rf_model.predict(X_new)[0]  # predicted class\n",
    "    pred_proba = rf_model.predict_proba(X_new)[0]  # array of probabilities\n",
    "\n",
    "    # Find the index of the predicted class\n",
    "    class_index = list(rf_model.classes_).index(pred)\n",
    "    probability = pred_proba[class_index]\n",
    "\n",
    "    print(f\"\\nPredicted Water Quality: {pred} (Probability: {probability:.2f})\")\n",
    "\n",
    "# Example usage with tuned Random Forest\n",
    "predict_water_rf(rf_grid.best_estimator_)\n"
   ]
